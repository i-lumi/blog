AA,GAAA,KAAA,OAAA;AACA,WAAA,SAAA,GAAA,KAAA,SAAA,CAAA,MAAA,CAAA,KAAA,gBAAA,CAAA,KAAA,CAAA,CAAA,WAAA,GAAA,CAAA,IAAA,OAAA,EAAA,WAAA,GAAA,OAAA,CAAA,CAAA;AACA,KALA;AAMA,IAAA,QANA,sBAMA;AACA,WAAA,WAAA,GAAA,KAAA,WAAA,GAAA,CAAA;AACA,WAAA,WAAA;AACA,KATA;AAUA,IAAA,OAVA,mBAUA,KAVA,EAUA;AACA,UAAA,IAAA,GAAA,KAAA,SAAA,CAAA,KAAA,CAAA;;AACA,UAAA,CAAA,IAAA,EAAA;AACA;AACA;;AAJA,UAKA,IALA,GAKA,IALA,CAKA,WALA,CAKA,IALA;;AAMA,UAAA,IAAA,IAAA,IAAA,CAAA,IAAA,CAAA,KAAA,QAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA,EAAA,KAAA,CAAA,CAAA,EAAA,CAAA,CAAA;AACA;AACA,KAnBA;AAoBA,IAAA,OApBA,mBAoBA,IApBA,EAoBA;AAAA,UACA,IADA,GACA,IADA,CACA,WADA,CACA,IADA;;AAEA,UAAA,IAAA,IAAA,IAAA,CAAA,IAAA,CAAA,KAAA,QAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA,EAAA,KAAA,CAAA,CAAA,EAAA,EAAA,CAAA;AACA;AACA;AAzBA;AA9BA,CAAA","sourcesContent":["<template>\r\n  <div class=\"custom-page archives-page\">\r\n    <div class=\"theme-vdoing-wrapper\">\r\n      <h1>\r\n        <img :src=\"currentBadge\" v-if=\"$themeConfig.titleBadge === false ? false : true\">\r\n        {{this.$page.title}}\r\n      </h1>\r\n      <ul>\r\n        <template\r\n         v-for=\"(item, index) in postsList\"\r\n        >\r\n          <li\r\n            class=\"year\"\r\n            v-if=\"(year = getYear(index)) !== getYear(index-1)\"\r\n            :key=\"index+$sortPostsByDate.length\"\r\n          >\r\n            <h2>{{year}}</h2>\r\n          </li>\r\n          <li\r\n            :key=\"index\"\r\n          >\r\n            <router-link :to=\"item.path\">\r\n              <span>\r\n                {{ getDate(item) }}\r\n              </span>\r\n              {{item.title}}\r\n            </router-link>\r\n          </li>\r\n        </template>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport debounce from 'lodash.debounce'\r\nimport { type } from '../util'\r\nimport TitleBadgeMixin from '../mixins/titleBadge'\r\n\r\nexport default {\r\n  mixins: [TitleBadgeMixin],\r\n  data() {\r\n    return {\r\n      postsList: [],\r\n\r\n      perPage: 80, // 每页长\r\n      currentPage: 1// 当前页\r\n    }\r\n  },\r\n  created() {\r\n    this.getPageData()\r\n  },\r\n  mounted() {\r\n    \r\n    window.addEventListener('scroll', debounce(() => {\r\n      if(this.postsList.length < this.$sortPostsByDate.length) {\r\n        const docEl = document.documentElement\r\n        const docBody = document.body\r\n        const scrollTop = docEl.scrollTop || docBody.scrollTop;\r\n        const clientHeight = docEl.clientHeight || docBody.clientHeight;\r\n        const scrollHeight = docEl.scrollHeight || docBody.scrollHeight;\r\n\r\n        if(scrollHeight > clientHeight && scrollTop + clientHeight >= scrollHeight-250) {\r\n          this.loadmore()\r\n        }\r\n      }\r\n      \r\n    }, 200))\r\n  },\r\n  methods: {\r\n    getPageData() {\r\n      const currentPage = this.currentPage\r\n      const perPage = this.perPage\r\n      this.postsList = this.postsList.concat(this.$sortPostsByDate.slice((currentPage-1)*perPage, currentPage*perPage))\r\n    },\r\n    loadmore() {\r\n      this.currentPage = this.currentPage+1\r\n      this.getPageData()\r\n    },\r\n    getYear(index) {\r\n      const item = this.postsList[index]\r\n      if (!item) {\r\n        return\r\n      }\r\n      const {frontmatter: { date }} = item\r\n      if (date && type(date) === 'string') {\r\n        return date.split(\" \")[0].slice(0, 4)\r\n      }\r\n    },\r\n    getDate(item) {\r\n      const {frontmatter: { date }} = item\r\n      if (date && type(date) === 'string') {\r\n        return date.split(\" \")[0].slice(5, 10)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang='stylus'>\r\n@require '../styles/wrapper.styl'\r\n\r\n\r\n.archives-page\r\n  .theme-vdoing-wrapper\r\n    @extend $vdoing-wrapper\r\n    position relative\r\n    @media (min-width $contentWidth + 80)\r\n      margin-top 1.5rem!important\r\n    ul,li\r\n      margin 0\r\n      padding 0\r\n    li\r\n      list-style none\r\n      &.year\r\n        position sticky\r\n        top $navbarHeight\r\n        background var(--mainBg)\r\n        z-index 1\r\n      &.year:not(:first-child)\r\n        margin-top 3.5rem\r\n      h2\r\n        margin-bottom .8rem\r\n        font-weight 400\r\n        padding .5rem 0\r\n      a\r\n        display block\r\n        color var(--textColor)\r\n        transition padding .3s\r\n        padding .5rem 2rem\r\n        line-height 1.2rem\r\n        &:hover\r\n          padding-left 2.5rem\r\n          color $accentColor\r\n          background lighten($accentColor, 90%)\r\n        @media (max-width $contentWidth + 80)\r\n          padding .5rem 1rem\r\n          font-weight: normal;\r\n          &:hover\r\n            padding-left 1.5rem\r\n        span \r\n          opacity .6\r\n          font-size .85rem\r\n          font-weight 400\r\n          margin-right .3rem\r\n    .loadmore\r\n      text-align center\r\n      margin-top 1rem\r\n      opacity .5\r\n\r\n.theme-mode-dark .archives-page .theme-vdoing-wrapper li a:hover,\r\n.theme-mode-read .archives-page .theme-vdoing-wrapper li a:hover\r\n  background var(--customBlockBg)\r\n\r\n\r\n.hide-navbar\r\n  .archives-page\r\n    .theme-vdoing-wrapper\r\n      li.year\r\n        top 0\r\n</style>\r\n"],"sourceRoot":"node_modules/vuepress-theme-vdoing/components"}]}